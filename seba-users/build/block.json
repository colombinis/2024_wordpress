{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "seba-users/clients",
  "version": "0.1.0",
  "title": "Seba Clients",
  "category": "widgets",
  "icon": "admin-users",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "order": {
      "type": "string",
      "enum": [
        "ASC",
        "DESC"
      ],
      "default": "ASC"
    },
    "posts_per_page": {
      "type": "string",
      "default": 5
    }
  },
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true
    },
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "textdomain": "seba-users",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}